# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Web to App' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Web to App
	pod 'SideMenu'
	pod 'Alamofire', '~> 4.5'
	pod 'Firebase/AdMob'
        pod 'Firebase/Messaging'
  target 'Web to AppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Web to AppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
